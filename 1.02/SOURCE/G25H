G25H:		CALL RA
		XOR AH, AH
		MOV AL, [OPCODE]
		SHL AX, 1
		SHL AX, 1
		ADD AX, OFFSET FLDTB
		MOV BX, AX
		MOV AL, [ARGS]
		CMP AL, 5
		JZ G25HR
		CMP AL, 2
		JNZ G25HF
		MOV AL, [ARGS+2]
		CMP AL, 3
		JZ G25HD
		CMP AL, 4
		JZ G25HQ
		CMP AL, 5
		JZ G25HT
G25HF:		MOV AL, 1
		JMP FAIL
G25HR:		INC BX
G25HT:		INC BX
G25HQ:		INC BX
G25HD:		MOV AL, [BX]
		MOV [OPCODE], AL
		CALL EXTEND
		JMP MMODRM
