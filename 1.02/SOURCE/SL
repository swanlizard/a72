SL:		PUSH CX
		MOV CX, [DI]
		JCXZ SL0
SLL:		PUSH CX
		PUSH SI
		INC DI
		INC DI
		XOR CH, CH
		MOV CL, [DI]
		INC CX
		CLD
	REPZ	CMPSB
		POP SI
		PUSHF
		ADD DI, CX
		POPF
		POP CX
		LOOPNZ SLL
		JNZ SL0
SL1:		MOV AX, [DI]
		POP CX
		CLC
		RET
SL0:		INC DI
		INC DI
		POP CX
		STC
		RET
