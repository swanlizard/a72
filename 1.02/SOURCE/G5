G5:		CALL RA
		AND BYTE PTR [FLAGS], 0E2H
		MOV AX, [ARGS]
		CMP AL, 4
		JNZ G5F0
		OR AH, AH
		JZ G5I0
		CMP AH, 1
		JZ G5I1
		CMP AH, 2
		JNZ G5F0
G5I1:		MOV AX, [PC]
		ADD AX, [VORG]
		INC AX
		INC AX
		SUB [IMM], AX
G5I0:		MOV AX, [IMM]
		CALL BITS
		JC G5F5
		RET
G5F0:		XOR AL, AL
		JMP FAIL
G5F5:		MOV AL, 5
		JMP FAIL
