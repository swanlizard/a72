WR:		OR BYTE PTR [OUTPUT], 0
		JZ WRE
		CMP BYTE PTR [FUNC], 2
		JNZ WR0
		XOR AX, AX
		MOV CX, 80H
		MOV DI, OFFSET ELEMENT
		CLD
	REPZ	STOSW
		OR WORD PTR [USIZE], 0
		JZ WRNU
WRUL:		MOV AH, 40H
		MOV CX, 100H
		MOV DX, OFFSET ELEMENT
		MOV BX, [OUTFILE]
		CMP CX, [USIZE]
		JNA WRUL1
		MOV CX, [USIZE]
WRUL1:		INT 21H
		JC WRE
		SUB [USIZE], CX
		JA WRUL
WRNU:		MOV AH, 40H
		XOR CH, CH
		MOV CL, [OUTPUT]
		MOV DX, OFFSET OUTPUT+1
		MOV BX, [OUTFILE]
		INT 21H
		JC WRE
WR0:		MOV WORD PTR [USIZE], 0
		CLC
WRE:		RET
