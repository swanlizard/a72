G1AH:		OR BYTE PTR [SI], 0
		JNZ G1AHR
		INC BYTE PTR [OPCODE]
		JMP SHORT G1AHE
G1AHR:		CALL RA
		CALL RA
		MOV AX, [ARGS]
		CMP AL, 55H
		JNZ G1AHF
		MOV AL, AH
		TEST AL, 0F0H
		JNZ G1AHF
		AND AL, 7
		OR [OPCODE], AL
G1AHE:		MOV AL, 0DEH
		XCHG AL, [OPCODE]
		MOV [MODRM], AL
		OR BYTE PTR [FLAGS], 10H
		RET
G1AHF:		MOV AL, 1
		JMP FAIL
