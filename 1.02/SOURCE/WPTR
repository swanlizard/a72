WPTR:		CMP AL, 0C0H
		JNC WPTR0
		PUSH AX
		OR BYTE PTR [WADJ], 0
		JZ WBPTR
		MOV AX, 6F77H
		STOSW
		MOV AX, 6472H
		STOSW
		JMP SHORT WSELF
WBPTR:		MOV AX, 7962H
		STOSW
		MOV AX, 6574H
		STOSW
WSELF:		MOV AX, 7020H
		STOSW
		MOV AX, 7274H
		STOSW
		MOV AL, 20H
		STOSB
		POP AX
WPTR0:		RET
