G04D:		PUSH SI
		MOV AL, [SI]
		SHR AL, 1
		SHR AL, 1
		AND AX, 0EH
		PUSH AX
		ADD AX, OFFSET _G04D
		MOV SI, AX
		MOV SI, [SI]
		MOV CL, [SI]
		INC SI
		XOR CH, CH
	REPZ	MOVSB
		MOV AL, 9
		STOSB
		POP AX
		POP SI
		SHR AL, 1
		CMP AL, 5
		JZ G04DF
		CMP AL, 4
		JZ G04DD
		CMP AL, 3
		JZ G04DF
		CMP AL, 2
		JZ G04DD
		LODSB
		CALL WPTR
		CALL WDISP
		JMP ENDL
G04DF:		MOV AL, [SI]
		CMP AL, 0C0H
		JC G04DM
		MOV AX, 6166H
		STOSW
		MOV AX, 2072H
		STOSW
G04DD:		LODSB
		CALL WDISP
		JMP ENDL
G04DM:		MOV AX, 7764H
		STOSW
		MOV AX, 726FH
		STOSW
		MOV AX, 2064H
		STOSW
		MOV AX, 7470H
		STOSW
		MOV AX, 2072H
		STOSW
		LODSB
		CALL WDISP
		JMP ENDL
