G16H:		XOR AL, AL
		MOV [FLAGS], AL
		MOV AL, [FUNC]
		CMP AL, 1
		JZ G16H1
		CMP AL, 2
		JZ G16H1
		RET
G16H1:		CLD
		LODSB
		XOR AH, AH
		MOV CX, AX
		JCXZ G16HF8
		PUSH CX
		PUSH SI
		MOV AL, [SI]
		CMP AL, 22H
		JZ G16HQ
		CMP AL, 27H
		JNZ G16HG
G16HQ:		INC SI
		SUB CX, 2
		JNA G16HF
G16HG:		MOV DI, OFFSET TEMP
	REPZ	MOVSB
		XOR AL, AL
		STOSB
		POP SI
		POP CX
		ADD SI, CX
		MOV AX, 3D00H
		MOV DX, OFFSET TEMP
		INT 21H
		JC G16HF
		MOV [INFILE], AX
		INC WORD PTR [INCLEV]
		CALL CALF
		PUSH SI
		MOV SI, OFFSET TEMP
		CALL INCF
		POP SI
		RET
G16HF:		MOV AL, 0BH
		JMP FAIL
G16HF8:		MOV AL, 8
		JMP FAIL
