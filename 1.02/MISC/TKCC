TKCC:		CLD
		PUSH AX
		PUSH DX
		PUSH DI
		INC DI
		MOV DX, DI
CCL:		LODSB
		MOV AH, AL
		OR AL, AL
		JZ CC0
		CMP AL, 0DH
		JZ CC0
		CMP AL, 21H
		JC CSKIP
		CMP AL, 22H
		JZ CQUO
		CMP AL, 27H
		JZ CQUO
		CMP AL, 2CH
		JZ CSKIP
		CMP AL, 2EH
		JZ CLTR
		CMP AL, 2FH
		JNA CSYM
		CMP AL, 39H
		JNA CLTR
		CMP AL, 3BH
		JZ CC0
		CMP AL, 3FH
		JNA CSYM
		CMP AL, 5AH
		JNA CLTR
		CMP AL, 5FH
		JZ CLTR
		CMP AL, 60H
		JNA CSYM
		CMP AL, 7AH
		JNA CLTR
		CMP AL, 7FH
		JNA CSYM
CSKIP:		CMP DI, DX
		JNZ CC0
		JMP SHORT CCL
CLTR:		STOSB
		JMP SHORT CCL
CQUO:		STOSB
		LODSB
		OR AL, AL
		JZ CC0
		CMP AL, 0DH
		JZ CC0
		CMP AL, AH
		JNZ CQUO
		INC SI
		JMP SHORT CC1
CSYM:		CMP DI, DX
		JNZ CC0
CC1:		STOSB
CC0:		DEC SI
		MOV AX, DI
		POP DI
		SUB AX, DX
		POP DX
		MOV [DI], AL
		CMP AL, 1
		POP AX
		RET
