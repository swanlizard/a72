BBBW:		MOV DX, 0B800H
		MOV ES, DX
		XOR AH, AH
		MOV DI, AX
		XCHG AL, AH
		SHR AX, 1
		SHR AX, 1
		SHR AX, 1
		SHL DI, 1
		SHL DI, 1
		SHL DI, 1
		ADD DI, AX
		XOR BP, BP
		AND AX, 2000H
		ADD DI, AX
		CMP AX, 2000H
		SBB BP, BP
		AND BP, 80
		MOV AX, CX
		SHR AX, 1
		SHR AX, 1
		ADD DI, AX
		AND CL, 3
		SHL CL, 1
		TEST BL, 3
		LAHF
		SHR BL, 1
		SHR BL, 1
		ROL AH, 1
		RCL AH, 1
		CMC
		ADC BL, 0
		CLD
		MOV DX, BX
BBBWLV:		MOV CH, DL
BBBWLH:		LODSB
		XOR AH, AH
		MOV BX, 0FF00H
		ROR AX, CL
		ROR BX, CL
		AND ES:[DI], BX
		OR ES:[DI], AX
		INC DI
		DEC CH
		JNZ BBBWLH
		XOR AH, AH
		MOV AL, DL
		SUB DI, AX
		XOR BP, 80
		XOR DI, 2000H
		ADD DI, BP
		DEC DH
		JNZ BBBWLV
		RET
