TKSC:		JC TKSC3
		XOR AX, AX
		MOV CL, [SI]
		XOR CH, CH
		INC SI
		JCXZ TKSC0
TKSC3:		CLD
		PUSH DX
TKSC1:		PUSH CX
		PUSH DI
		INC AX
		MOV DX, SI
		XOR CH, CH
		MOV CL, [SI]
		INC CX
	REPZ	CMPSB
		POP DI
		JZ TKSC2
		ADD SI, CX
		POP CX
		LOOP TKSC1
		POP DX
TKSC0:		XOR AX, AX
		STC
		RET
TKSC2:		MOV SI, DX
		POP CX
		POP DX
		CLC
		RET
