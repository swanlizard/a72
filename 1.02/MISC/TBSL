TBSL:		CLD
		MOV CX, [DI]
		XOR DX, DX
		JCXZ TBSL0
		XOR AH, AH
TBSL2:		PUSH CX
		PUSH SI
		INC DX
		INC DI
		INC DI
		MOV AL, [DI]
		MOV CX, AX
		INC CX
	REPZ	CMPSB
		POP SI
		JZ TBSL1
		ADD DI, CX
		POP CX
		LOOP TBSL2
TBSL0:		INC DI
		INC DI
		STC
		RET
TBSL1:		MOV AX, [DI]
		POP CX
		CLC
		RET
