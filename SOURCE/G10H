G10H:		CALL RA
		CALL RA
		CALL WA
		MOV AX, [ARGS]
		CMP AL, 11H
		JZ G10HR
		CMP AL, 12H
		JZ G10HM
		CMP AL, 21H
		JZ G10HM
		XOR AL, AL
		JMP FAIL
G10HR:		OR BYTE PTR [WADJ], 0
		JZ G10HM
		TEST AH, 0FH
		JZ G10HAR
		TEST AH, 0F0H
		JZ G10HRA
G10HM:		MOV AL, [WADJ]
		OR AL, 86H
		MOV [OPCODE], AL
		JMP MMODRM
G10HAR:		MOV CL, 4
		SHR AH, CL
G10HRA:		MOV AL, AH
		AND AL, 7
		OR AL, 90H
		MOV [OPCODE], AL
		RET
