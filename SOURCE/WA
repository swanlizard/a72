WA:		MOV AL, [ARGS+2]
		MOV AH, AL
		MOV CL, 4
		SHR AH, CL
		AND AX, 0F0FH
		JZ WAZ
		OR AL, AL
		JZ WALZ
		OR AH, AH
		JZ WAHZ
		CMP AL, AH
		JZ WAHZ
WA0:		MOV AL, 6
		JMP FAIL
WALZ:		CMP AL, AH
		JZ WAZ
		MOV AL, AH
WAHZ:		CMP AL, 2
		JA WA0
		DEC AX
WAZ:		MOV [WADJ], AL
		TEST BYTE PTR [FLAGS], 2
		JZ WAX
		OR [FLAGS], AL
WAX:		RET
