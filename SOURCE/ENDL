SINGD:		DEC DI
ENDL:		CALL TABS
		MOV AX, [PC]
		ADD AX, [VORG]
		OR BYTE PTR [SEGPREF], 0
		JZ SKSPR
		INC AX
SKSPR:		PUSH AX
		MOV AL, AH
		CALL HALX
		POP AX
		CALL HALX
		PUSH SI
		MOV CX, SI
		SUB CX, BX
		MOV SI, BX
		DEC CX
		INC SI
		OR BYTE PTR [SEGPREF], 0
		JZ DOPS
		DEC CX
		INC SI
DOPS:		MOV AL, 20H
		STOSB
		LODSB
		CALL HALX
		LOOP DOPS
		POP SI
		MOV AX, 0A0DH
		STOSW
		RET
