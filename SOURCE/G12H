G12H:		CALL RA
		CALL RA
		MOV AX, [ARGS]
		MOV DL, [ARGS+2]
		CMP AL, 11H
		JZ G12HR
		CMP AL, 14H
		JZ G12HI
G12HF:		XOR AL, AL
		JMP FAIL
G12HR:		CMP AH, 2
		JNZ G12HF
		TEST DL, 2
		JZ G12HF
		MOV AL, 0EEH
		MOV [OPCODE], AL
		TEST DL, 20H
		JZ G12H0
		INC BYTE PTR [OPCODE]
G12H0:		RET
G12HI:		MOV AL, 0E6H
		MOV [OPCODE], AL
		TEST DL, 20H
		JZ G12H0
		INC BYTE PTR [OPCODE]
		RET
