G0EH:		CALL RA
		CALL RA
		MOV AX, [ARGS]
		CMP AL, 14H
		JZ G0EHRM
		CMP AL, 24H
		JZ G0EHRM
		XOR AL, AL
		JMP FAIL
G0EHRM:		MOV AX, [IMM]
		CMP AX, 3FH
		JA G0EHF
		MOV AH, AL
		ROR AL, 1
		ROR AL, 1
		ROR AL, 1
		AND AX, 707H
		ROL AH, 1
		ROL AH, 1
		ROL AH, 1
		OR [OPCODE], AX
		AND BYTE PTR [FLAGS], 0FCH
		JMP MMODRM
G0EHF:		MOV AL, 7
		JMP FAIL
