G26H:		CALL RA
		MOV AL, [ARGS]
		CMP AL, 2
		JNZ G26HF
		OR BYTE PTR [MODRM], 8
		MOV AL, [ARGS+2]
		CMP AL, 2
		JZ G26HW
		CMP AL, 3
		JZ G26HD
		CMP AL, 4
		JZ G26HQ
G26HF:		XOR AL, AL
		JMP FAIL
G26HW:		MOV AL, 0DFH
		MOV [OPCODE], AL
		JMP MMODRM
G26HD:		MOV AL, 0DBH
		MOV [OPCODE], AL
		JMP MMODRM
G26HQ:		MOV AL, 0DDH
		MOV [OPCODE], AL
		JMP MMODRM
