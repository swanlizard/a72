G2:		CALL RA
		OR BYTE PTR [SI], 0
		JZ G2MP
		CALL WA
		MOV AL, [OPCODE]
		MOV [MODRM], AL
		MOV AL, [WADJ]
		OR AL, 0D0H
		MOV [OPCODE], AL
		CLD
		LODSW
		CMP AX, 3101H
		JZ G2N
		CMP AX, 6302H
		JNZ G2S
		LODSB
		CMP AL, 6CH
		JNZ G2F
		ADD BYTE PTR [OPCODE], 2
G2N:		JMP MMODRM
G2F:		XOR AL, AL
		JMP FAIL
G2MP:		MOV AL, 8
		JMP FAIL
G2S:		DEC SI
		DEC SI
		CALL VAL
		JC G2F
		MOV [IMM], AX
		OR AH, AH
		JNZ G2F
		OR BYTE PTR [FLAGS], 2
		AND BYTE PTR [OPCODE], 0EFH
		JMP MMODRM
