G17H:		XOR AL, AL
		MOV [FLAGS], AL
		PUSH SI
		MOV SI, BP
		CALL SCREG
		POP SI
		JC G17H1
		CMP AL, 10H
		JC G17HFC
		AND AL, 3
		SHL AL, 1
		SHL AL, 1
		SHL AL, 1
		OR AL, 26H
		MOV [SEGPREF], AL
		OR BYTE PTR [FLAGS], 40H
		LODSW
		CMP AX, 3A01H
		JNZ G17HFC
		RET
G17HFC:		MOV AL, 0CH
		JMP FAIL
G17H1:		MOV AX, [SI]
		OR AL, AL
		JZ G17HF3
		CMP AX, 3A01H
		JNZ NOCOL
		INC SI
		INC SI
NOCOL:		CMP BYTE PTR [FUNC], 1
		JNZ G17H2
		PUSH SI
		MOV SI, BP
		MOV DI, OFFSET SYMBS
		CALL SL
		JNC G17HFA
		MOV CL, [SI]
		XOR CH, CH
		INC CX
		CLD
	REPZ	MOVSB
		MOV AX, [PC]
		ADD AX, [VORG]
		STOSW
		POP SI
		INC WORD PTR [SYMBS]
G17H2:		MOV SP, [STK]
		JMP ASML
G17HF3:		MOV AL, 3
		JMP FAIL
G17HFA:		MOV AL, 0AH
		JMP FAIL
