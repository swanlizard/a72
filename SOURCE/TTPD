TTPD:		MOV DX, 1
		MOV AL, [OPCODE]
		CMP AL, 0DBH
		JZ FLDDW
		CMP AL, 0DFH
		JZ FLDDD
		JMP SHORT FLDDQ
FILDD:		MOV DX, 1
		MOV AL, [OPCODE]
		CMP AL, 0DBH
		JZ FLDDW
		TEST BYTE PTR [MODRM], 20H
		JZ FLDDD
		JMP SHORT FLDDQ
FLDD:		MOV AL, [MODRM]
		CMP AL, 0C0H
		JNC FLDDR
		MOV DX, 2
		MOV AL, [OPCODE]
		CMP AL, 0DDH
		JZ FLDDW
		CMP AL, 0D9H
		JZ FLDDD
FLDDQ:		INC DX
FLDDW:		INC DX
FLDDD:		MOV [WADJ], DX
		MOV AL, [MODRM]
		CALL WPTR
		CALL WDISP
		JMP ENDL
FLDDR:		CALL WFST
		JMP ENDL
