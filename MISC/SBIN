SBIN:		PUSH AX
		PUSH CX
		PUSH DX
		PUSH DI
		CLD
		CMP AX, 1
		JA SBIN1
		PUSH AX
		MOV AH, 1
		OR AL, 30H
		XCHG AL, AH
		MOV [DI], AX
		POP AX
		RET
SBIN1:		MOV CX, 17
SBIN2:		ROL AX, 1
		TEST AL, 1
		LOOPZ SBIN2
		MOV DX, AX
		MOV AL, CL
		STOSB
SBIN3:		MOV AL, DL
		AND AL, 1
		OR AL, 30H
		STOSB
		ROL DX, 1
		LOOP SBIN3
		POP DI
		POP DX
		POP CX
		POP AX
		RET
