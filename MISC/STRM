STRM:		PUSH AX
		PUSH CX
		PUSH DX
		PUSH SI
		PUSH DI
		JCXZ STRM0
		CMP [SI], AL
		JC STRM0
		MOV DX, AX
		ADD DX, CX
		OR DH, DH
		JNZ STRM0
		CMP [SI], DL
		JC STRM0
		MOV DI, SI
		ADD DI, AX
		MOV AL, [SI]
		SUB AL, DL
		SUB [SI], CL
		ADD SI, DX
		MOV CX, AX
		INC SI
		INC DI
		CLD
	REPZ	MOVSB
STRM0:		POP DI
		POP SI
		POP DX
		POP CX
		POP AX
		RET
