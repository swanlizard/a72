HAXL:		SUB AX, 3030H
		CMP AL, 9
		JNA HAXL2
		AND AL, 0CFH
		JZ HAXL1
		CMP AL, 6
		JA HAXL1
		ADD AL, 9
		JMP SHORT HAXL2
HAXL1:		XOR AL, AL
HAXL2:		XCHG AL, AH
		CMP AL, 9
		JNA HAXL4
		AND AL, 0CFH
		JZ HAXL3
		CMP AL, 6
		JA HAXL3
		ADD AL, 9
		JMP SHORT HAXL4
HAXL3:		XOR AL, AL
HAXL4:		AAD 16
		RET
