TBRM:		OR DX, DX
		JZ TBRM0
		CMP DX, [DI]
		JA TBRM0
		JZ TBRM1
		PUSH DI
		MOV CX, [DI]
		XOR BH, BH
		INC DI
		INC DI
		DEC DX
		JZ TBRM2
TBRM3:		MOV BL, [DI]
		LEA DI, [BX+DI+3]
		DEC DX
		LOOPNZ TBRM3
TBRM2:		MOV SI, DI
TBRM4:		MOV BL, [DI]
		LEA DI, [BX+DI+3]
		LOOP TBRM4
		MOV CX, DI
		MOV DI, SI
		MOV BL, [DI]
		LEA SI, [BX+SI+3]
		SUB CX, SI
		CLD
	REPZ	MOVSB
		POP DI
TBRM1:		DEC WORD PTR [DI]
		CLC
		RET
TBRM0:		STC
		RET
