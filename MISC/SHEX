SHEX:		CLD
		CMP AX, 9
		JA SHEX1
		PUSH AX
		MOV AH, 1
		OR AL, 30H
		XCHG AL, AH
		MOV [DI], AX
		POP AX
		RET
SHEX1:		PUSH AX
		PUSH CX
		PUSH DX
		PUSH DI
		MOV CX, 5
SHEX2:		ROL AX, 1
		ROL AX, 1
		ROL AX, 1
		ROL AX, 1
		TEST AL, 15
		LOOPZ SHEX2
		MOV DX, AX
		MOV AL, CL
		STOSB
SHEX3:		MOV AL, DL
		AND AL, 15
		CMP AL, 9
		JNA SHEX4
		ADD AL, 7
SHEX4:		ADD AL, 30H
		STOSB
		ROL DX, 1
		ROL DX, 1
		ROL DX, 1
		ROL DX, 1
		LOOP SHEX3
		POP DI
		POP DX
		POP CX
		POP AX
		RET
