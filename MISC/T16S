T16S:		MOV DX, 0B800H
		MOV ES, DX
		MOV DX, AX
		XOR DH, DH
		XOR BX, BX
		TEST DH, 1
		JZ T16S1
		MOV BX, 2000H
T16S1:		SHL DX, 1
		SHL DX, 1
		XOR AL, AL
		MOV DI, AX
		SHR DI, 1
		SHL AX, 1
		ADD DI, AX
		ADD DI, DX
		ADD DI, BX
		CLD
		MOV CX, 16
T16SL:		MOVSW
		MOVSW
		SUB DI, 4
		XOR DI, 2000H
		TEST DI, 2000H
		JNZ T16S2
		ADD DI, 80
T16S2:		LOOP T16SL
		RET
