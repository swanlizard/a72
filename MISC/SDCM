SDCM:		PUSH AX
		PUSH DX
		PUSH BX
		MOV BX, 10
		PUSH AX
SDCM1:		XOR DX, DX
		DIV BX
		INC DI
		OR AX, AX
		JNZ SDCM1
		POP AX
		PUSH DI
SDCM2:		XOR DX, DX
		DIV BX
		ADD DL, 30H
		MOV [DI], DL
		DEC DI
		OR AX, AX
		JNZ SDCM2
		POP AX
		SUB AX, DI
		MOV [DI], AL
		POP BX
		POP DX
		POP AX
		RET
