LSPR:		CLD
		PUSH DI
		PUSHF
		MOV DI, OFFSET ELEMENT
		MOV AL, 9
		STOSB
		XOR AL, AL
		POPF
		JC LFPR
		XCHG AL, [SEGPREF]
		SUB AL, 26H
		CALL WSR
		MOV AL, 3AH
		STOSB
		JMP SHORT WTABS
LFPR:		XCHG AL, [FPREF]
		MOV AX, 6177H
		STOSW
		MOV AX, 7469H
		STOSW
WTABS:		PUSH BP
		PUSH DX
		MOV BP, OFFSET ELEMENT-1
		CALL TABS
		POP DX
		POP BP
		MOV AX, [PC]
		ADD AX, [VORG]
		PUSH AX
		MOV AL, AH
		CLD
		CALL HALX
		POP AX
		CALL HALX
		MOV AL, 20H
		STOSB
		MOV AL, DL
		CALL HALX
		MOV AX, 0A0DH
		STOSW
		POP CX
		PUSH SI
		MOV SI, BP
		SUB CX, BP
	REPZ	MOVSB
		MOV CX, DI
		MOV SI, OFFSET ELEMENT
		SUB CX, SI
		MOV DI, BP
		INC DI
	REPZ	MOVSB
		POP SI
		RET
