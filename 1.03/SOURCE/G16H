G16H:		XOR AL, AL
		MOV [FLAGS], AL
		MOV AL, [FUNC]
		CMP AL, 1
		JZ G16H1
		CMP AL, 2
		JZ G16H1
		RET
G16H1:		CALL CC
		JC G16HF8
		MOV CX, BX	
		PUSH SI
		CMP AL, 22H
		JZ G16HQ
		CMP AL, 27H
		JNZ G16HG
G16HQ:		INC SI
		SUB CX, 2
		JNA G16HF
G16HG:		MOV DI, OFFSET TEMP
		CLD
	REPZ	MOVSB
		XOR AL, AL
		STOSB
		POP SI
		ADD SI, BX
		MOV AX, [EOB]
		SUB AX, SI
		JNA G16HNS
		NEG AX
		CWD
		MOV CX, DX
		MOV DX, AX
		MOV AX, 4201H
		MOV BX, [INFILE]
		INT 21H
G16HNS:		MOV AX, 3D00H
		MOV DX, OFFSET TEMP
		INT 21H
		JC G16HF
		MOV [INFILE], AX
		INC WORD PTR [INCLEV]
		CALL CALF
		PUSH SI
		MOV SI, OFFSET TEMP
		CALL INCF
		POP SI
		RET
G16HF:		MOV AL, 0BH
		JMP FAIL
G16HF8:		MOV AL, 8
		JMP FAIL
