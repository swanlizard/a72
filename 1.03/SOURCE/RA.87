RA:		CALL CC
		CALL SCREG
		JNC RAR
		CALL BORW
		JNC RAD
		CALL RE
		MOV [IMM], AX
		MOV DH, 2
		OR BYTE PTR [FLAGS], 3
		TEST DL, 3
		JNZ RAIW
		OR AH, AH
		JZ RAIB
		CALL BITS
		JC RAIW
RAIB:		AND BYTE PTR [FLAGS], 0FEH
		XOR DH, DH
RAIW:		MOV AL, DL
		MOV DL, 4
		JMP WARG
RAR:		CMP AL, 20H
		JNC RAST
		CALL GSPR
		JNC RAD
		MOV DX, 203H
		CMP AL, 10H
		JNC RARS
		MOV DL, 1
		CMP AL, 8
		JNC RARS
		MOV DH, 1
RARS:		AND AL, 7
		JMP WARG
RAR1:		MOV AL, 1
		JMP FAIL
RAST:		MOV DX, 505H
		AND AL, 7
		JMP WARG
RAD:		XOR AX, AX
		XOR CX, CX
		XOR DX, DX
RADS:		MOV DH, AL
		INC SI
RADL:		CALL CC
		JC RAR1
		CMP AL, 5DH
		JZ RADX
		CMP AL, 2BH
		JZ RADS
		CMP AL, 2DH
		JZ RADS
		CALL SCREG
		JNC RADR
		CALL GV
		JC RAR1
		ADD [DISP], AX
		JMP SHORT RADL
RADR:		MOV CL, 1
		CMP AL, 14
		JZ SID
		CMP AL, 15
		JZ DID
		CMP AL, 13
		JZ BPD
		CMP AL, 11
		JZ BXD
RAD0:		XOR AL, AL
		JMP FAIL
RAD2:		MOV AL, 2
		JMP FAIL
RAD8:		MOV AL, 8
		JMP FAIL
RADD:		MOV AL, 0DH
		JMP FAIL
SID:		SHL CL, 1
DID:		SHL CL, 1
BPD:		SHL CL, 1
BXD:		TEST CH, CL
		JNZ RAD2
		CMP DH, 2DH
		JZ RADD
		OR CH, CL
		MOV DH, 0FFH
		JMP SHORT RADL
RADX:		OR DH, DH
		JZ RAD8
		INC SI
		MOV AX, [DISP]
		OR AX, AX
		JZ RADN
		CALL BITS
		SALC
		AND AL, 4
		OR AL, 8
		OR [FLAGS], AL
RADN:		TEST DL, 3
		JZ RADB
		OR BYTE PTR [FLAGS], 0CH
RADB:		MOV AL, CH
		MOV BX, OFFSET RM
		XLATB
		TEST AL, 0F0H
		JNZ RAD2
		MOV DH, [WADJ]
		MOV DL, 2
		CMP AL, 0EH
		JNZ WARG
		OR BYTE PTR [FLAGS], 0CH
WARG:		TEST BYTE PTR [ARGS], 0FH
		JZ NARGS
		MOV CL, 4
		ROL AL, CL
		ROL DX, CL
NARGS:		XCHG DH, AL
		OR [ARGS+2], AL
		OR [ARGS], DX
		RET
