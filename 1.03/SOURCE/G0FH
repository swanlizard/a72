G0FH:		CALL RA
		CALL RA
		CALL WA
		MOV AX, [ARGS]
		CMP AL, 11H
		JZ G0FHR
		CMP AL, 12H
		JZ G0FHR
		CMP AL, 21H
		JZ G0FHR
		CMP AL, 41H
		JZ G0FHRI
		CMP AL, 42H
		JZ G0FHMI
		XOR AL, AL
		JMP FAIL
G0FHR:		MOV AL, [WADJ]
		OR AL, 84H
		MOV [OPCODE], AL
		JMP MMODRM
G0FHRI:		AND AH, 7
		JNZ G0FHMI
		MOV AL, [WADJ]
		OR AL, 0A8H
		MOV [OPCODE], AL
		RET
G0FHMI:		MOV AL, [WADJ]
		OR AL, 0F6H
		MOV [OPCODE], AL
		JMP MMODRM
