G12H:		CALL GPN
		JMP GPA
GPN:		CALL CC
		CALL SCREG
		JC GPNI
		CMP AL, 0AH
		JNZ GPNF
		OR BYTE PTR [OPCODE], 8
		RET
GPNF:		XOR AH, AH
		JMP FAIL
GPNI:		CALL RE
		OR AH, AH
		JNZ GPNF7
		MOV [IMM], AX
		OR BYTE PTR [FLAGS], 2
		RET
GPNF7:		MOV AL, 7
		JMP FAIL
GPA:		CALL CC
		CALL SCREG
		JC GPNF
		CMP AL, 10H
		JC GPAL
		INC BYTE PTR [OPCODE]
GPAL:		AND AL, 7
		JNZ GPNF
		RET
