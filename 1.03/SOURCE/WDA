WDA:		MOV BX, 10
WV:		CLD
		CMP AX, BX
		JNC WV0
		CMP AX, 9
		JA WV0
		OR AL, 30H
		STOSB
		RET
WV0:		PUSH AX
		XOR CX, CX
WV1:		XOR DX, DX
		DIV BX
		INC CX
		OR AX, AX
		JNZ WV1
		POP AX
		PUSH SI
		MOV SI, DI
WV2:		XOR DX, DX
		DIV BX
		XCHG AX, DX
		CMP AL, 9
		JNA WV3
		ADD AL, 7
WV3:		ADD AL, 30H
		STOSB
		XCHG AX, DX
		OR AX, AX
		JNZ WV2
		XCHG AX, DX
		CMP AL, 39H
		JNA WV4
		MOV AL, 30H
		STOSB
		INC CX
WV4:		SHR CL, 1
		PUSH DI
		XCHG SI, DI
WV5:		DEC SI
		MOV AL, [DI]
		XCHG AL, [SI]
		STOSB
		LOOP WV5
		POP DI
		POP SI
		RET
