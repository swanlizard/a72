WPTR:		CMP AL, 0C0H
		JNC WPTR0
		PUSH AX
		PUSH SI
		MOV AX, [WADJ]
		CMP AX, 10H
		JZ WPTRW
		SHL AX, 1
		ADD AX, OFFSET SIZES
		JMP SHORT WPTRA
WPTRW:		MOV AX, OFFSET SIZES+2
WPTRA:		MOV SI, AX
		LODSW
		MOV SI, AX
		LODSB
		XOR AH, AH
		MOV CX, AX
		CLD
	REPZ	MOVSB
		MOV AL, 20H
		MOV SI, OFFSET _PTR+1
		STOSB
		MOVSW
		MOVSB
		STOSB
		POP SI
		POP AX
WPTR0:		RET
