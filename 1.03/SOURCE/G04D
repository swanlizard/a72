G04D:		PUSH SI
		MOV AL, [SI]
		SHR AL, 1
		SHR AL, 1
		AND AX, 0EH
		PUSH AX
		ADD AX, OFFSET _G04D
		MOV SI, AX
		MOV SI, [SI]
		MOV CL, [SI]
		INC SI
		XOR CH, CH
	REPZ	MOVSB
		MOV AL, 9
		STOSB
		POP AX
		POP SI
		SHR AL, 1
		CMP AL, 5
		JZ G04DF
		CMP AL, 4
		JZ G04DD
		CMP AL, 3
		JZ G04DF
		CMP AL, 2
		JZ G04DD
		LODSB
		CALL WPTR
		CALL WDISP
		JMP ENDL
G04DF:		MOV AL, [SI]
		PUSH SI
		CMP AL, 0C0H
		JC G04DM
		MOV SI, OFFSET _FAR+1
		MOV AL, 20H
		MOVSW
		MOVSB
		STOSB
		POP SI
G04DD:		LODSB
		CALL WDISP
		JMP ENDL
G04DM:		MOV SI, OFFSET _DWORD+1
		MOV AL, 20H
		MOVSW
		MOVSW
		MOVSB
		STOSB
		MOV SI, OFFSET _PTR+1
		MOVSW
		MOVSB
		STOSB
		POP SI
		LODSB
		CALL WDISP
		JMP ENDL
