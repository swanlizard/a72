RE:		PUSH BP
		XOR AX, AX
		XOR DX, DX
		XOR BP, BP
		DEC SI
RES:		MOV DH, AL
		INC SI
REL:		CALL CC
		JC REE
		CMP AL, 2BH
		JZ RES
		CMP AL, 2DH
		JZ RES
		CALL SCREG
		JNC REC
		CALL GV
		JC REE
		ADD BP, AX
		JMP SHORT REL
REE:		MOV AX, BP
		POP BP
		RET
REC:		MOV AL, 0CH
		JMP FAIL
