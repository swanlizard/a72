SL:		PUSH CX
		MOV CX, [DI]
		INC DI
		INC DI
		JCXZ SL0
SLL:		PUSH CX
		XOR CH, CH
		MOV CL, [DI]
		INC DI
		CMP CX, BX
		JNZ SL2
		CLD
		PUSH SI
	REPZ	CMPSB
		POP SI
		JZ SL1
SL2:		ADD DI, CX
		INC DI
		INC DI
		POP CX
		LOOP SLL
SL0:		POP CX
		STC
		RET
SL1:		MOV AX, [DI]
		POP CX
		POP CX
		CLC
		RET
