G22H:		CALL EXTEND
		CALL CC
		JNC G22HG
		OR BYTE PTR [MODRM], 0D1H
		RET
G22HG:		CALL RA
		MOV AL, [ARGS]
		CMP AL, 5
		JZ G22HD
		CMP AL, 2
		JNZ G22HF
		MOV AL, [ARGS+2]
		CMP AL, 3
		JZ G22HD
		CMP AL, 4
		JZ G22HQ
G22HF:		MOV AL, 1
		JMP FAIL
G22HQ:		OR BYTE PTR [OPCODE], 4
G22HD:		JMP MMODRM
