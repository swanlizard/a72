GETFN:		CLD
GFN1:		LODSB
		CMP AL, 20H
		JNA GFN1
		CMP CX, SI
		JNC GFN2
		RET
GFN2:		DEC SI
GFN3:		LODSB
		CMP AL, 20H
		JNA GFN5
		CMP AL, 61H
		JC GFN4
		CMP AL, 7AH
		JA GFN4
		AND AL, 0DFH
GFN4:		STOSB
		JMP SHORT GFN3
GFN5:		XOR AL, AL
		STOSB
		DEC SI
		CMP CX, SI
		RET
