GV:		CALL CC
		JC GVA
		CMP DH, 0FFH
		JZ GVA
		CMP AL, 24H
		JZ GVC
		CMP AL, 22H
		JZ GVQ
		CMP AL, 27H
		JZ GVQ
		CALL VAL
		JC GVL
GVV:		CMP DH, 2DH
		JNZ GVX
		NEG AX
GVX:		MOV DH, 0FFH
		CLC
		RET
GVA:		STC
		RET
GVC:		INC SI
		MOV AX, [PC]
		ADD AX, [VORG]
		OR DL, 2
		JMP SHORT GVV
GVQ:		PUSH WORD PTR [SI+1]
		ADD SI, BX
		MOV AL, BL
		CMP AL, 3
		JC GV8
		CMP AL, 4
		JA GV7
		POP AX
		JNC GVV
		XOR AH, AH
		JMP SHORT GVV
GV4:		MOV AL, 4
		JMP FAIL
GV7:		MOV AL, 7
		JMP FAIL
GV8:		MOV AL, 8
		JMP FAIL
GVL:		PUSH DI
		MOV DI, OFFSET _OFFSET
		CALL SW
		CALL CC
		JC GV8
		MOV AX, [PC]
		ADD AX, [VORG]
		CMP BYTE PTR [FUNC], 2
		JNZ GVE
		MOV DI, OFFSET SYMBS
		CALL SL
		JC GV4
GVE:		POP DI
		ADD SI, BX
		OR DL, 1
		JMP SHORT GVV
