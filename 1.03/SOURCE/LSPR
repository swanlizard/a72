LSPR:		XOR DL, DL
		XCHG DL, [SEGPREF]
		OR DL, DL
		JZ LSPR0
		CLD
		PUSH DI
		MOV DI, OFFSET TEMP+1
		MOV AL, 9
		STOSB
		MOV AL, DL
		CALL WSR
		MOV AL, 3AH
		STOSB
		PUSH BP
		PUSH DX
		MOV BP, OFFSET TEMP
		CALL TABS
		POP DX
		POP BP
		MOV AX, [PC]
		ADD AX, [VORG]
		CALL WRAX
		MOV AL, 20H
		STOSB
		MOV AL, DL
		CALL HALX
		MOV AX, 0A0DH
		STOSW
		POP CX
		PUSH SI
		MOV SI, BP
		INC SI
		SUB CX, SI
	REPZ	MOVSB
		MOV CX, DI
		MOV SI, OFFSET TEMP+1
		SUB CX, SI
		MOV DI, BP
		INC DI
	REPZ	MOVSB
		POP SI
LSPR0:		RET
