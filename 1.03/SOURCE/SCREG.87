SCREG:		CMP BX, 2
		JNZ SCREG0
		PUSH AX
		MOV AX, [SI]
		CMP AX, 7473H
		JZ SCREG1
		MOV DI, OFFSET REGS
		CLD
		PUSH CX
		MOV CX, 14H
	REPNZ	SCASW
		POP CX
		POP AX
		JNZ SCREG0
		MOV AX, DI
		SUB AX, OFFSET REGS+2
		SHR AL, 1
		INC SI
		INC SI
		CLC
		RET
SCREG0:		STC
		RET
SCREG1:		POP AX
		INC SI
		INC SI
		CALL CC
		JC SCREG2
		CMP AL, 28H
		JNZ SCREG2
		INC SI
		CALL CC
		JC SCREG3
		CALL VAL
		JC SCREG3
		CMP AX, 7
		JA SCREG3
		OR AL, 20H
		PUSH AX
		CALL CC
		JC SCREG3
		CMP AL, 29H
		JNZ SCREG3
		POP AX
		INC SI
		CLC
		RET
SCREG2:		MOV AL, 20H
		CLC
		RET
SCREG3:		MOV AL, 0FH
		JMP FAIL
