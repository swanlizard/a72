		MOV CX, 14H
	REPNZ	SCASW
		JNZ SCREG0
		MOV AL, 13H
		SUB AL, CL
		CLC
		RET
SCREG0:		DEC SI
		DEC SI
		STC
		RET
SCREG1:		CALL CC
		JC SCREG2
		CMP AL, 28H
