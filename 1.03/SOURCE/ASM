ASM:		XOR AX, AX
		MOV [OUTPUT], AL
		MOV [STK], SP
		MOV DI, OFFSET FLAGS
		MOV CX, 14
		CLD
	REPZ	STOSB
		CALL CC
		MOV BP, SI
ASML:		MOV SP, [STK]
		CALL CC
		JC ASM0
		MOV DI, OFFSET I8086
		CALL SL
		JC NOGOOD
		ADD SI, BX
		MOV [OPCODE], AL
		OR BYTE PTR [FLAGS], 20H
		XOR AL, AL
		XCHG AL, AH
		SHL AX, 1
		ADD AX, OFFSET IHDL
		MOV BX, AX
		CALL [BX]
		CALL CC
		JNC ASMX
ASM0:		OR BYTE PTR [FLAGS], 0
		JNZ WRITE
		JMP WRD
NOGOOD:		CALL SCREG
		JC BAD
		CALL GSPR
		JNC ASML
		MOV AL, 0CH
		JMP FAIL
BAD:		CMP BP, SI
		JC BADF3
		MOV CX, BX
		TEST BYTE PTR [FUNC], 1
		JZ BADX
		MOV DI, OFFSET SYMBS
		CALL SL
		JNC BADFA
		CLD
		MOV AL, CL
		STOSB
	REPZ	MOVSB
		MOV AX, [PC]
		ADD AX, [VORG]
		STOSW
		INC WORD PTR [SYMBS]
BADX:		ADD SI, CX
		CALL CC
		JC BADF3
		CMP AL, 3AH
		JNZ ASML
		INC SI
		JMP SHORT ASML
BADF3:		MOV AL, 3
		JMP FAIL
BADFA:		MOV AL, 0AH
		JMP FAIL
ASMX:		MOV AL, 9
		JMP FAIL
WRITE:		CLD
		MOV DI, OFFSET OUTPUT+1
		MOV SI, OFFSET PREFIX
		MOV DL, [FLAGS]
		LODSB
		TEST DL, 80H
		JZ NOPREF
		STOSB
NOPREF:		LODSB
		TEST DL, 40H
		JZ NOSPR
		STOSB
NOSPR:		LODSB
		TEST DL, 20H
		JZ NOOPC
		STOSB
NOOPC:		LODSB
		TEST DL, 10H
		JZ NOMOD
		STOSB
NOMOD:		LODSW
		TEST DL, 8
		JZ WNDISP
		TEST DL, 4
		JZ WBDISP
		STOSB
		MOV AL, AH
WBDISP:		STOSB
WNDISP:		LODSW
		TEST DL, 2
		JZ WNIMM
		TEST DL, 1
		JZ WBIMM
		STOSB
		MOV AL, AH
WBIMM:		STOSB
WNIMM:		MOV AX, DI
		SUB AX, OFFSET OUTPUT+1
		MOV [OUTPUT], AL
WRD:		XOR AH, AH
		MOV AL, [OUTPUT]
		ADD [PC], AX
		CLC
		RET
