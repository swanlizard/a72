DISASM:		CLD
		XOR AX, AX
		MOV [WADJ], AX
		MOV [STK], SP
		MOV BP, DI
		STOSB
		MOV BX, SI
		INC SI
		MOV [SEGPREF], AL
		MOV [FPREF], AL
DSAS:		MOV SP, [STK]
		CLD
		MOV AL, 9
		STOSB
		LODSB
		MOV [OPCODE], AL
		AND AL, 1
		MOV CL, 4
		SHL AL, CL
		MOV [WADJ], AL
		MOV AL, [OPCODE]
		XOR AH, AH
		SHL AX, 1
		SHL AX, 1
		ADD AX, OFFSET BIN86
		CALL CNVTB
		CALL LFPR
		CALL LSPR
		MOV AX, SI
		SUB AX, BX
		DEC AX
		ADD [PC], AX
		MOV AX, DI
		SUB AX, BP
		DEC AX
		ADD [BP], AL
		RET
