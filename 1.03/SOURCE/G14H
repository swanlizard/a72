G14H:		XOR AX, AX
		MOV [UD], AX
		CALL CC
		JC G14HF
		MOV DI, OFFSET OUTPUT+1
G14HL:		CMP AL, 3FH
		JZ G14HU
		CALL G14HS
		CMP AL, 22H
		JZ G14HQ
		CMP AL, 27H
		JZ G14HQ
		PUSH DI
		CALL RE
		POP DI
		MOV CL, [OPCODE]
		CMP CH, CL
		JA G14HF7
		DEC CX
		MOV DX, 1
		SHL DX, CL
		CLD
		STOSB
		OR CL, CL
		JZ G14HB
		MOV AL, AH
		STOSB
G14HB:		SUB DX, 2
		JNA G14HC
		MOV CX, DX
		RCL AH, 1
		SALC
	REPZ	STOSB
		JMP SHORT G14HC
G14HQ:		CMP BYTE PTR [OPCODE], 1
		JA G14HF
		MOV AH, AL
		MOV CX, BX
		SUB CX, 2
		JNA G14HF
		INC SI
		CLD
	REPZ	MOVSB
		LODSB
		CMP AL, AH
		JNZ G14HF
G14HC:		CALL CC
		JNC G14HL
		MOV AX, DI
		SUB AX, OFFSET OUTPUT+1
		MOV [OUTPUT], AL
		MOV AX, [UD]
		ADD [PC], AX
		XOR AL, AL
		MOV [FLAGS], AL
		RET
G14HF:		MOV AL, 1
		JMP FAIL
G14HF7:		MOV AL, 7
		JMP FAIL
G14HU:		MOV CL, [OPCODE]
		MOV AX, 1
		DEC CX
		SHL AX, CL
		ADD [UD], AX
		INC SI
		JMP SHORT G14HC
G14HS:		XOR CX, CX
		XCHG CX, [UD]
		JCXZ G14HS0
		PUSH AX
		XOR AL, AL
		CLD
	REPZ	STOSB
		POP AX
G14HS0:		RET
