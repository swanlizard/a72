G18H:		XOR AL, AL
		MOV [FLAGS], AL
		MOV AL, [FUNC]
		CMP AL, 1
		JZ G18H1
		CMP AL, 2
		JZ G18H1
		RET
G18H1:		CALL CC
		JC G18HF8
		MOV CX, BX	
		PUSH SI
		CMP AL, 22H
		JZ G18HQ
		CMP AL, 27H
		JNZ G18HG
G18HQ:		INC SI
		SUB CX, 2
		JNA G18HF
G18HG:		MOV DI, OFFSET TEMP
		CLD
	REPZ	MOVSB
		XOR AL, AL
		STOSB
		POP SI
		ADD SI, BX
		MOV AX, 3D00H
		MOV DX, OFFSET TEMP
		INT 21H
		JC G18HF
		PUSH AX
		MOV AX, [EOB]
		SUB AX, SI
		JNA G18HNS
		NEG AX
		CWD
		MOV CX, DX
		MOV DX, AX
		MOV AX, 4201H
		MOV BX, [INFILE]
		INT 21H
G18HNS:		POP AX
		MOV [INFILE], AX
		INC WORD PTR [INCLEV]
		CALL CALF
		PUSH SI
		MOV SI, OFFSET TEMP
		CALL INCF
		POP SI
		RET
G18HF:		MOV AL, 0BH
		JMP FAIL
G18HF8:		MOV AL, 8
		JMP FAIL
