G0DH:		CALL RA
		CMP BYTE PTR [ARGS+2], 1
		JZ G0DHF
		MOV AX, [ARGS]
		CMP AL, 1
		JZ G0DHR
		CMP AL, 2
		JZ G0DHM
		CMP AL, 3
		JNZ G0DHF
		MOV AL, AH
		AND AL, 3
		SHL AL, 1
		SHL AL, 1
		SHL AL, 1
		OR AL, 6
		TEST BYTE PTR [OPCODE], 8
		JZ G0DH8
		OR AL, 1
G0DH8:		MOV [OPCODE], AL
		RET
G0DHR:		MOV AL, AH
		AND AL, 7
		OR [OPCODE], AL
		RET
G0DHM:		MOV AX, 30FFH
		TEST BYTE PTR [OPCODE], 8
		JZ G0DHB
		MOV AX, 8FH
G0DHB:		MOV [OPCODE], AX
		JMP MMODRM
G0DHF:		XOR AL, AL
		JMP FAIL
