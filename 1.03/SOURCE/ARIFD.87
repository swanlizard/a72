ARIFD:		MOV AL, [MODRM]
		CMP AL, 0C0H
		JNC ARIFDR
		XOR AX, AX
		CMP BYTE PTR [OPCODE], 0DCH
		CMC
		ADC AL, 2
		MOV [WADJ], AX
		MOV AL, [MODRM]
		CALL WPTR
		CALL WDISP
		JMP ENDL
ARIFDR:		CMP BYTE PTR [OPCODE], 0DCH
		JC ARIFD0
		CALL WFST
		MOV AL, 2CH
		STOSB
		XOR AL, AL
		CALL WFST
		JMP ENDL
ARIFD0:		XOR AL, AL
		CALL WFST
		MOV AL, 2CH
		STOSB
		MOV AL, [MODRM]
		CALL WFST
		JMP ENDL
