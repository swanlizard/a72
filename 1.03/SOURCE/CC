CC:		XOR BX, BX
CCM:		MOV AL, [BX+SI]
		OR AL, AL
		JZ CCT
		CMP AL, 0AH
		JZ CCT
		CMP AL, 0DH
		JZ CCT
		CMP AL, 21H
		JC CCK
		CMP AL, 22H
		JZ CCQ
		CMP AL, 24H
		JZ CCS
		CMP AL, 27H
		JZ CCQ
		CMP AL, 28H
		JZ CCS
		CMP AL, 29H
		JZ CCS
		CMP AL, 2BH
		JZ CCS
		CMP AL, 2CH
		JZ CCK
		CMP AL, 2DH
		JZ CCS
		CMP AL, 3AH
		JZ CCS
		CMP AL, 3BH
		JZ CCT
		CMP AL, 3FH
		JZ CCS
		CMP AL, 41H
		JC CCL
		CMP AL, 5BH
		JZ CCS
		CMP AL, 5DH
		JZ CCS
		CMP AL, 5AH
		JA CCL
		OR AL, 20H
		MOV [BX+SI], AL
CCL:		INC BX
		JMP SHORT CCM
CCT:		OR BX, BX
		JNZ CCE
		STC
		RET
CCQ:		MOV AH, AL
CCN:		INC BX
		MOV AL, [BX+SI]
		CMP AL, AH
		JNZ CCN
		INC BX
		CLC
		RET
CCS:		OR BX, BX
		JNZ CCE
		INC BX
CCE:		MOV AL, [SI]
		CLC
		RET
CCK:		OR BX, BX
		JNZ CCE
		INC SI
		JMP SHORT CCM
