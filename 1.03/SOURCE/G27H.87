G27H:		CALL CC
		JNC G27HR
		INC BYTE PTR [OPCODE]
		JMP SHORT G27HE
G27HR:		CALL RA
		CALL RA
		MOV AX, [ARGS]
		CMP AL, 55H
		JNZ G27HF
		MOV AL, AH
		TEST AL, 0F0H
		JNZ G27HF
		AND AL, 7
		OR [OPCODE], AL
G27HE:		MOV AL, 0DEH
		XCHG AL, [OPCODE]
		MOV [MODRM], AL
		OR BYTE PTR [FLAGS], 10H
		RET
G27HF:		MOV AL, 1
		JMP FAIL
