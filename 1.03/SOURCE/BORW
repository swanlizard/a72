BORW:		CMP AL, 62H
		JZ BYTEP
		CMP AL, 77H
		JZ WORDP
		CMP AL, 64H
		JZ DWORDP
		CMP AL, 5BH
		JZ BRACP
BORW0:		STC
		RET
BYTEP:		MOV DI, OFFSET _BYTE
		JMP SHORT SPTR
WORDP:		MOV DI, OFFSET _WORD
		JMP SHORT SPTR
DWORDP:		MOV DI, OFFSET _DWORD
SPTR:		CALL SW
		JC BORW0
		MOV AL, [DI]
		MOV [WADJ], AL
		CALL CC
		JC BORW8
		MOV DI, OFFSET _PTR
		CALL SW
		CALL CC
		JC BORW8
		CALL SCREG
		JC SPTRR
		CALL GSPR
		JC BORW1
SPTRR:		CALL CC
		CMP AL, 5BH
		JNZ BORW1
BRACP:		CLC
		RET
BORW1:		MOV AL, 1
		JMP FAIL
BORW8:		MOV AL, 8
		JMP FAIL
