G2:		CALL RA
		CALL WA
		MOV AL, [OPCODE]
		MOV [MODRM], AL
		MOV AL, [WADJ]
		OR AL, 0D0H
		MOV [OPCODE], AL
		CALL CC
		JC G2N
		MOV AX, [SI]
		ADD SI, BX
		CMP BX, 2
		JZ G2R
		CMP BX, 1
		JNZ G2F
		CMP AL, 31H
		JZ G2N
G2F:		XOR AL, AL
		JMP FAIL
G2R:		CMP AX, 6C63H
		JNZ G2F
		OR BYTE PTR [OPCODE], 2
G2N:		JMP MMODRM
