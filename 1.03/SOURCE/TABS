TABS:		MOV DX, BP
		INC DX
		MOV CX, DI
		DEC CX
		SUB CX, BP
		JNA TABS6
		MOV AX, CX
		PUSH AX
		PUSH DI
		DEC DI
		STD
		MOV AL, 0DH
	REPNZ	SCASB
		POP DI
		POP AX
		JNZ TABS2
		INC CX
		INC CX
		SUB AX, CX
		JNA TABS6
		ADD DX, CX
TABS2:		MOV CX, AX
		PUSH SI
		MOV SI, DX
		XOR DX, DX
		CLD
TABS1:		LODSB
		CMP AL, 9
		JZ TABS3
		INC DX
		LOOP TABS1
		JMP SHORT TABS4
TABS3:		MOV AX, DX
		AND AX, 7
		NEG AX
		ADD AX, 8
		ADD DX, AX
		LOOP TABS1
TABS4:		POP SI
		MOV AL, 9
		SHR DX, 1
		SHR DX, 1
		SHR DX, 1
		MOV CX, 5
		SUB CX, DX
		JNA TABS5
	REPZ	STOSB
TABS5:		MOV AH, 3BH
		STOSW
TABS6:		RET
