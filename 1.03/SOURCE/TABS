TABS:		CLD
		MOV CX, DI
		DEC CX
		SUB CX, BP
		JNA TABS0
		PUSH SI
		MOV SI, BP
		INC SI
		XOR DX, DX
TABS1:		LODSB
		CMP AL, 9
		JZ TABS2
		INC DX
		LOOP TABS1
		JMP SHORT TABS3
TABS2:		MOV AX, DX
		AND AX, 7
		NEG AX
		ADD AX, 8
		ADD DX, AX
		LOOP TABS1
TABS3:		POP SI
		MOV AL, 9
		MOV CL, 3
		SHR DX, CL
		INC CX
		INC CX
		SUB CX, DX
		JNA TABS4
	REPZ	STOSB
TABS4:		MOV AH, 3BH
		STOSW
TABS0:		RET
