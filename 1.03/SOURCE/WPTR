WPTR:		CMP AL, 0C0H
		JNC WPTR0
		PUSH AX
		PUSH SI
		OR BYTE PTR [WADJ], 0
		JZ WBPTR
		MOV SI, OFFSET _WORD+1
		JMP SHORT WSELF
WBPTR:		MOV SI, OFFSET _BYTE+1
WSELF:		MOVSW
		MOVSW
		MOV SI, OFFSET _PTR+1
		MOV AL, 20H
		STOSB
		MOVSW
		MOVSB
		STOSB
		POP SI
		POP AX
WPTR0:		RET
