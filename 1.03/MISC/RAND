RAND:		PUSH CX
		PUSH DX
		PUSH AX
		MOV AX, [RANDS]
		MOV DX, AX
		MOV CL, 7
		SHL DX, CL
		XOR AX, DX
		MOV DX, AX
		ADD CL, 2
		SHR DX, CL
		XOR AX, DX
		MOV DX, AX
		DEC CL
		SHL DX, CL
		XOR AX, DX
		XOR DX, DX
		MOV [RANDS], AX
		POP CX
		OR CX, CX
		JZ RAND0
		DIV CX
RAND0:		MOV AX, DX
		POP DX
		POP CX
		RET
		DB 0A0H
RANDS		DW 0DEADH
