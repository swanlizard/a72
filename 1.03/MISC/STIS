STIS:		OR BYTE PTR [SI], 0
		JZ STIS0
		CMP [DI], AL
		JC STIS0
		OR BYTE PTR [DI], 0
		JNZ STIS1
		PUSH CX
		PUSH SI
		PUSH DI
		XOR CH, CH
		MOV CL, [SI]
		CLD
		INC CX
	REPZ	MOVSB
		POP DI
		POP SI
		POP CX
STIS0:		RET
STIS1:		PUSH CX
		PUSH DX
		PUSH SI
		PUSH DI
		XOR CH, CH
		XOR DH, DH
		MOV CL, [SI]
		MOV DL, [DI]
		ADD DX, CX
		CMP DX, 100H
		JNC STISE
		CMP AL, [DI]
		JZ STISA
		DEC DI
		ADD DI, DX
		MOV SI, DI
		ADD DI, CX
		MOV CX, DX
		SUB CX, AX
		STD
	REPZ	MOVSB
		POP DI
		POP SI
		MOV CL, [SI]
		PUSH SI
		PUSH DI
STISA:		ADD [DI], CL
		ADD DI, AX
		INC SI
		INC DI
		CLD
	REPZ	MOVSB
STISE:		POP DI
		POP SI
		POP DX
		POP CX
		RET
